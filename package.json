{
  "name": "ytdt-claims-pipeline",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server/server.js",
  "packageManager": "pnpm@10.21.0",
  "scripts": {
    "start": "node --max-old-space-size=8192 src/server/index.js",
    "dev": "nodemon --max-old-space-size=8192 src/server/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install && node scripts/sync-version.js",
    "sync-version": "node scripts/sync-version.js"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "csv-stringify": "^6.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "google-auth-library": "^10.4.1",
    "googleapis": "^128.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.2.0",
    "multer": "^2.0.2",
    "mysql2": "^3.6.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.3.0",
    "vitest": "^2.1.0"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
