{
  "name": "ytdt-claims-pipeline",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server/server.ts",
  "packageManager": "pnpm@10.21.0",
  "scripts": {
    "start": "NODE_OPTIONS='--max-old-space-size=8192' tsx src/server/index.ts",
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' nodemon --exec tsx src/server/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install && tsx scripts/sync-version.ts",
    "type-check": "tsc --noEmit --project tsconfig.app.json && tsc --noEmit --project tsconfig.test.json",
    "sync-version": "tsx scripts/sync-version.ts",
    "danger": "danger ci"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "csv-stringify": "^6.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "google-auth-library": "^10.4.1",
    "googleapis": "^128.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.2.0",
    "multer": "^2.0.2",
    "mysql2": "^3.6.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@commitlint/lint": "^19.0.0",
    "@commitlint/load": "^19.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "danger": "^11.3.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^2.1.0"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
