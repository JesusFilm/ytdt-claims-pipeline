# API
NODE_ENV=development
PORT=3000
MONGODB_URI=mongodb://localhost:27017/ytdt-pipeline
BASE_URL=http://localhost:3000

# Pipeline settings
PIPELINE_TIMEOUT_MINUTES=30             # Timeout for pipeline processing in minutes
ML_API_ENDPOINT=http://localhost:3001   # Optional ML API endpoint
SKIP_VPN=true                           # Uncomment for local testing without VPN

# MySQL
MYSQL_HOST=your_host_here
MYSQL_USER=your_user_here
MYSQL_PASSWORD=your_password_here
MYSQL_DATABASE=jfp_analytics_prod

# VPN
VPN_CONFIG_FILE=./config/vpn/client.ovpn
VPN_AUTH_FILE=./config/vpn/auth.txt

# Google Drive (requires valid Service Account Key in config/service-account-key.json)
GOOGLE_DRIVE_NAME=youtube_exports

# Slack integration
SLACK_BOT_TOKEN=xoxb-your-token-here
SLACK_SIGNING_SECRET=your-signing-secret-here
SLACK_CHANNEL=#youtube-data-chat
